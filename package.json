{
  "name": "@jod/urataidot-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^24.0.0"
  },
  "license": "EUPL-1.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "prettier . --write",
    "preview": "vite preview",
    "test:unit": "vitest --run",
    "test:e2e": "playwright test",
    "prepare": "husky",
    "translations:check": "node ./scripts/translations/analyze-translations.js",
    "translations:fetch": "node ./scripts/translations/fetch-translations.js",
    "translations:push": "tolgee push",
    "translations:tag": "node ./scripts/translations/manage-tags.js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "wip",
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "perf",
          "test",
          "revert",
          "build",
          "ci",
          "chore"
        ]
      ]
    }
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^5.2.1",
    "@jod/design-system": "https://github.com/Opetushallitus/jod-design-system/releases/download/rel-20260218-2/jod-design-system-0.0.0.tgz",
    "@react-pdf/renderer": "^4.3.2",
    "cva": "^1.0.0-beta.4",
    "emoji-datasource-apple": "^16.0.0",
    "emoji-datasource-facebook": "^16.0.0",
    "emoji-datasource-google": "^16.0.0",
    "emoji-datasource-twitter": "^16.0.0",
    "emoji-picker-react": "^4.18.0",
    "html-react-parser": "^5.2.11",
    "i18next": "^25.8.5",
    "i18next-chained-backend": "^5.0.2",
    "i18next-http-backend": "^3.0.2",
    "i18next-resources-to-backend": "^1.2.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.5.4",
    "react-router": "^7.12.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.3.1",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@lavamoat/allow-scripts": "^3.4.1",
    "@lavamoat/preinstall-always-fail": "^2.1.1",
    "@playwright/test": "^1.58.2",
    "@singlestore/eslint-plugin-react-hooks-disable-import": "^1.0.2",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/react": "^16.3.1",
    "@tolgee/cli": "^2.15.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-sonarjs": "^3.0.5",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.0",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^4.0.18",
    "xlsx": "file:vendor/xlsx-0.20.3.tgz"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "@vitejs/plugin-react-swc>@swc/core": true,
      "vite>esbuild": true
    }
  }
}
